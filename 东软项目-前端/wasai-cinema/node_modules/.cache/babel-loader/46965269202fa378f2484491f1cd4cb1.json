{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport { workerAdd } from \"@/api/worker\";\nexport default {\n  data: function data() {\n    return {\n      FilmForm: {\n        name: null,\n        pwd: null,\n        releaseTime: null,\n        type: \"其他\",\n        region: \"其他\",\n        info: null,\n        duration: null\n      }\n    };\n  },\n  methods: {\n    //确认添加员工\n    onSubmit: function onSubmit() {\n      var _this = this;\n\n      if (this.FilmForm.staff_name == null || this.FilmForm.staff_pwd == null || this.FilmForm.staff_nickname == null || this.FilmForm.staff_phone == null) {\n        this.$message.error(\"请填写用户名,密码,昵称,和电话\");\n      } else {\n        if (this.FilmForm.staff_name == \"\" || this.FilmForm.staff_pwd == \"\" || this.FilmForm.staff_nickname == \"\" || this.FilmForm.staff_phone == \"\") {\n          this.$message.error(\"检测到空白数据\");\n        } else {\n          var vo = {\n            username: this.FilmForm.staff_name,\n            password: this.FilmForm.staff_pwd,\n            phone: this.FilmForm.staff_phone,\n            department: this.FilmForm.staff_dept,\n            entry: this.FilmForm.entry,\n            gender: this.FilmForm.staff_sex,\n            nickname: this.FilmForm.staff_nickname\n          };\n          workerAdd(vo).then(function (res) {\n            if (res.code == 200) {\n              if (res.data.success == \"success\") {\n                _this.$message({\n                  message: \"添加成功\",\n                  type: res.data.success\n                });\n              } else {\n                _this.$message.error(\"添加失败,请重试并查看字符\");\n              }\n            } else {\n              _this.$message.error(\"请求失败\");\n            }\n          });\n        }\n      }\n    }\n  }\n};","map":{"version":3,"mappi