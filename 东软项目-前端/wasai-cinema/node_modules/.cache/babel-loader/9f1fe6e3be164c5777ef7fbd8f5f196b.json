{"ast":null,"code":"import \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.string.iterator.js\";\nimport \"core-js/modules/web.dom-collections.iterator.js\";\nimport \"core-js/modules/web.url.js\";\nimport \"core-js/modules/web.url-search-params.js\";\nimport \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/es.array.splice.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport { workerAll, DeleteOneWorker } from \"@/api/worker\";\nimport { ModifySelf } from \"@/api/user\";\nimport { parseTime } from \"@/plugins/DateTran\"; //日期格式转化\n\nexport default {\n  data: function data() {\n    return {\n      //个人设置信息单\n      imageUrl: null,\n      dynamicValidateForm: {\n        domains: [{\n          value: null\n        }],\n        email: null //邮箱\n\n      },\n      ruleForm: {\n        pass: null\n      },\n      //密码\n      form: {\n        phone: null,\n        dept: null,\n        entry: null,\n        nickname: null,\n        //昵称\n        sex: null,\n        // 性别\n        desc: null //个人介绍\n\n      },\n      fileName: null,\n      //折叠面板的展开序列\n      activeNames: [],\n      //正式的客服数组\n      userlist: [],\n      //点击修改客服后保存索引\n      UpdateIndex: -1,\n      //修改客服信息的面板可见开关 \n      UpdateFormVisible: false,\n      //删除客服的具体开关\n      DeleteVisible: false\n    };\n  },\n  methods: {\n    //客服信息头像更新规则\n    beforeAvatarUpload: function beforeAvatarUpload(file) {\n      var isJPG = file.type === 'image/jpeg';\n      var isLt2M = file.size / 1024 / 1024 < 2;\n\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n\n      return isJPG && isLt2M;\n    },\n    //头像处理\n    handleAvatarSuccess: function handleAvatarSuccess(res, file) {\n      console.log(res.data.fileName);\n      this.fileName = res.data.fileName;\n      this.imageUrl = URL.createObjectURL(file.raw);\n    },\n    //更改客服基本信息\n    onSubmit2: function onSubmit2() {\n      var _this = this;\n\n      if (this.form.status == \"1\") {\n        var status = 1;\n      }\n\n      if (this.form.status == \"0\") {\n        var status = 0;\n      }\n\n      var data = {\n        status: status,\n        password: this.ruleForm.pass,\n        avatar: null,\n        nickname: this.form.name,\n        email: this.dynamicValidateForm.email,\n        birthday: this.form.date1,\n        gender: this.form.sex,\n        info: this.form.desc,\n        id: this.userlist[this.UpdateIndex].id\n      };\n      console.log(data);\n\n      if (this.ruleForm.pass != null) {\n        if (data.password.length < 6) {\n          this.$message.error(\"请输入6位以上的密码\");\n        } else {\n          ModifySelf(data).then(function (res) {\n            if (res.code == 200) {\n              if (res.data.success == \"success\") {\n                _this.$message({\n                  message: res.data.msg,\n                  type: res.data.success\n                });\n\n                _this.UpdateFormVisible = false;\n              } else {\n                _this.$message.error(\"更新失败\");\n              }\n            } else {\n              _this.$message.error(\"请求失败\");\n            }\n          });\n        }\n      } else {\n        ModifySelf(data).then(function (res) {\n          if (res.code == 200) {\n            if (res.data.success == \"success\") {\n              _this.$message({\n                message: res.data.msg,\n                type: res.data.success\n              });\n\n              _this.UpdateFormVisible = false;\n              window.location.href = \"#/admin_panel/order_manage\";\n              window.location.href = \"#/admin_panel/user_manage\";\n            } else {\n              _this.$message.error(\"更新失败\");\n            }\n          } else {\n            _this.$message.error(\"请求失败\");\n          }\n        });\n      }\n    },\n    //删除客服接口\n    DeleteOneUser: function DeleteOneUser() {\n      var _this2 = this;\n\n      DeleteOneWorker(this.userlist[this.UpdateIndex].id).then(function (res) {\n        if (res.code == 200) {\n          if (res.data.success == \"success\") {\n            _this2.$message({\n              message: \"删除成功\",\n              type: res.data.success\n            });\n\n            _this2.DeleteVisible = false;\n\n            _this2.userlist.splice(_this2.UpdateIndex, 1);\n          } else {\n            _this2.$message.error(\"删除失败\");\n          }\n        } else {\n          _this2.$message.error(\"请求失败\");\n        }\n      });\n    },\n    //弹出更改客服信息按钮\n    UpdateUserData: function UpdateUserData(index) {\n      this.UpdateFormVisible = true;\n      this.UpdateIndex = index;\n    },\n    //样式改变，不要动\n    changePad: function changePad(n) {\n      if (this.activeNames.indexOf(n) == -1) {\n        this.activeNames.push(n);\n      } else {\n        this.activeNames.splice(this.activeNames.indexOf(n), 1);\n      }\n    },\n    //删除某一个客服显示弹窗\n    DeleteUser: function DeleteUser(index) {\n      this.DeleteVisible = true;\n      this.UpdateIndex = index;\n    },\n    //查询所有的客服信息并加入userlist\n    QueryAlluser: function QueryAlluser() {\n      var _this3 = this;\n\n      workerAll().then(function (res) {\n        if (res.code == 200) {\n          _this3.userlist = res.data;\n\n          for (var i = 0; i < _this3.userlist.length; i++) {\n            _this3.userlist[i].createTime = parseTime(new Date(_this3.userlist[i].createTime));\n          }\n        }\n      });\n    }\n  },\n  created: function created() {\n    //查询所有客服并且放入客服表\n    this.QueryAlluser();\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;