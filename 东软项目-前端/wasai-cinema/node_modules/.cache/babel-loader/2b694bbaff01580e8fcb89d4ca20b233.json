me: \"客服3\",\n        sex: \"男\",\n        id: \"384311abxy\",\n        mycomment: [{\n          time: \"2021/11/17\",\n          content: \"XXXX\",\n          satisfy: \"一般\"\n        }, {\n          time: \"2020/5/12\",\n          content: \"XXxxXX\",\n          satisfy: \"一般\"\n        }]\n      }, {\n        pic: require(\"../assets/img/nim.png\"),\n        name: \"客服3\",\n        sex: \"男\",\n        id: \"384311abxy\",\n        mycomment: [{\n          time: \"2021/2/1\",\n          content: \"XXXXXXX\",\n          satisfy: \"满意\"\n        }, {\n          time: \"2020/5/19\",\n          content: \"XXxXX\",\n          satisfy: \"满意\"\n        }]\n      }],\n      //用户评论\n      user_comment: [],\n      //我的评论,接口输入\n      Mycomment: {},\n      //我的评论,筛选后\n      Mycomment_new_list: {}\n    };\n  },\n  methods: {\n    ShowMyComment: function ShowMyComment(n) {\n      console.log(n);\n      this.state_index = n;\n      this.My_comment_visible = true;\n    },\n    ShowStateComment: function ShowStateComment(n) {\n      console.log(n);\n      this.state_index = n;\n      this.comment_state_member_visible = true;\n    },\n    changeSatisfyCss: function changeSatisfyCss(item) {\n      if (item.type == \"非常满意\") {\n        return this.exe;\n      }\n\n      if (item.type == \"一般\") {\n        return this.just;\n      }\n\n      if (item.type == \"不满意\") {\n        return this.awful;\n      }\n    },\n    //查询员工接口和我的评价并且放入state_member\n    QueryWorkerApi: function QueryWorkerApi() {\n      var _this = this;\n\n      QueryWorker().then(function (res) {\n        if (res.code == 200) {\n          _this.state_member = res.data;\n          QueryWorkerMycomment().then(function (res) {\n            if (res.code == 200) {\n              _this.Mycomment = res.data;\n            }\n          });\n        }\n      });\n    },\n    //查询影院评论接口并且放入user_comment\n    QueryCineme: function QueryCineme() {\n      var _this2 = this;\n\n      QueryCinemeComment().then(function (res) {\n        if (res.code == 200) {\n          for (var i = 0; i < res.data.length; i++) {\n            if (res.data[i].user != null) {\n              _this2.user_comment.push(res.data[i]);\n            }\n          } //转化其中显示的日期\n\n\n          for (var i = 0; i < _this2.user_comment.length; i++) {\n            _this2.user_comment[i].create_time = parseTime(new Date(_this2.user_comment[i].create_time), \"{y}-{m}-{d}\");\n          }\n\n          ;\n        }\n      });\n    }\n  },\n  computed: {\n    Mycomment_new: function Mycomment_new() {\n      this.Mycomment_new_list = undefined;\n      this.Mycomment_new_list = new Array();\n\n      for (var j = 0; j < this.Mycomment.length; j++) {\n        if (this.state_member[this.state_index].id == this.Mycomment[j].wid) {\n          this.Mycomment_new_list.push(this.Mycomment[j]);\n        }\n      }\n\n      return this.Mycomment_new_list;\n    }\n  },\n  beforeCreate: function beforeCreate() {\n    console.log(\"BeforeCreate\");\n    this.$store.state.navnum = 3;\n    this.$store.state.color4.color = \"blue\";\n    this.$store.state.color1.color = \"#5b5d63\";\n    this.$store.state.color2.color = \"#5b5d63\";\n    this.$store.state.color3.color = \"#5b5d63\";\n  },\n  mounted: function mounted() {\n    //查询评论接口\n    this.QueryCineme(); //查询员工列表和我的评论接口\n\n    this.QueryWorkerApi();\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqFA;AACA,+C,CAAA;;AACA;AACAA,MADA,kBACA;AACA;AAEAC;AAAAC;AAAA,OAFA;AAGAC;AAAAD;AAAAE;AAAA,OAHA;AAIAC,4BAJA;AAKAC,yCALA;AAMAC,+BANA;AAOA;AACAC;AAAAC;AAAAC;AAAAC;AAAA,OARA;AASAC;AAAAH;AAAAC;AAAAC;AAAA,OATA;AAUAE;AAAAJ;AAAAC;AAAAC;AAAA,OAVA;AAWA;AACAG,oBAZA;AAaAC,qBACA;AACAC,6CADA;AAEAC,mBAFA;AAGAC,gBAHA;AAIAC,wBAJA;AAKAC,oBACA;AACAC,0BADA;AAEAnB,yBAFA;AAGAE;AAHA,SADA,EAMA;AACAiB,2BADA;AAEAnB,0BAFA;AAGAE;AAHA,SANA;AALA,OADA,EAmBA;AACAY,6CADA;AAEAC,mBAFA;AAGAC,gBAHA;AAIAC,wBAJA;AAKAC,oBACA;AACAC,0BADA;AAEAnB,4BAFA;AAGAE;AAHA,SADA,