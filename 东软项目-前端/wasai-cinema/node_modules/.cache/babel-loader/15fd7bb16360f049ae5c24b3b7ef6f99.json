abel\"><span style=\"color:red\">*</span>电话号码</div><el-input placeholder=\"请输入电话\" class=\"inputBox\" v-model=\" StaffForm.staff_phone\"></el-input></div>\r\n      <!-- 部门选框 -->\r\n      <div><div class=\"inputLabel\" style=\"margin-right:10px\"><span style=\"color:red\">*</span>入职部门</div>\r\n       <el-radio v-model=\"StaffForm.staff_dept\" label=\"客服部\">客服部</el-radio><el-radio v-model=\"StaffForm.staff_dept\" label=\"宣传部\">宣传部</el-radio>\r\n       <el-radio v-model=\"StaffForm.staff_dept\" label=\"运营部\">运营部</el-radio><el-radio v-model=\"StaffForm.staff_dept\" label=\"策划部\">策划部</el-radio>\r\n       <el-radio v-model=\"StaffForm.staff_dept\" label=\"人事部\">人事部</el-radio><el-radio v-model=\"StaffForm.staff_dept\" label=\"监督部\">监督部</el-radio>\r\n      </div>\r\n      <div><div class=\"inputLabel\"><span style=\"color:red\">*</span>头像</div>\r\n      <el-upload\r\n       class=\"upload-demo\" drag action=\"https://jsonplaceholder.typicode.com/posts/\" multiple style=\"margin-left:80px\">\r\n      <i class=\"el-icon-upload\"></i>\r\n        <div class=\"el-upload__text\">将文件拖到此处，或<em>点击上传</em></div>\r\n        <div class=\"el-upload__tip\" slot=\"tip\">只能上传jpg/png文件,且不超过500kb</div>\r\n</el-upload></div>\r\n      <!-- 按钮 -->\r\n      <el-form-item>\r\n    <el-button type=\"primary\" @click=\"onSubmit\">立即创建</el-button>\r\n    <el-button>取消</el-button>\r\n  </el-form-item>\r\n      </el-form>\r\n<!-- 底部 -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return{\r\n      StaffForm:{\r\n          staff_name:'',\r\n          staff_pwd:'',\r\n          staff_nickname:'',\r\n          staff_sex:'男',\r\n          staff_phone:'',\r\n          staff_dept:'',\r\n          staff_img:'',\r\n      }\r\n        \r\n    }\r\n  },\r\n  methods:{\r\n    onSubmit() {\r\n        console.log('submit!');\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.outbox{\r\n  overflow: auto;\r\n  background-color: aqua;\r\n  height:760px;\r\n  padding-le