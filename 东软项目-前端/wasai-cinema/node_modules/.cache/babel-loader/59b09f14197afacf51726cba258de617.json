{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport { QueryWorker, QueryCinemeComment, QueryWorkerMycomment, UploadWorkerMycomment, UploadLeaveMessage, UploadLeaveMessageReply } from \"@/api/comment\";\nimport { parseTime } from \"@/plugins/DateTran\"; //日期格式转化\n\nexport default {\n  data: function data() {\n    return {\n      //影院留言表单\n      form_comment: {\n        content: \"\"\n      },\n      //对员工评价表单\n      form_comment_state_member: {\n        content: \"\",\n        satisfy: \"一般\"\n      },\n      //添加评论显示\n      comment_visible: false,\n      //员工评价显示\n      comment_state_member_visible: false,\n      //我对员工评价\n      My_comment_visible: false,\n      //满意度样式\n      exe: {\n        excellent: true,\n        justso: false,\n        bad: false\n      },\n      just: {\n        excellent: false,\n        justso: true,\n        bad: false\n      },\n      awful: {\n        excellent: false,\n        justso: false,\n        bad: true\n      },\n      //   存放员工顺序，让其显示不同按钮下的各个员工评论\n      state_index: 0,\n      state_member: [],\n      //用户评论\n      user_comment: [],\n      //我的评论,接口输入\n      Mycomment: [],\n      //我的评论,筛选后\n      Mycomment_new_list: {}\n    };\n  },\n  methods: {\n    //上传影院留言接口\n    UploadMyLM: function UploadMyLM(vo) {\n      var _this = this;\n\n      UploadLeaveMessageReply(vo).then(function (res) {\n        if (res.code == 200) {\n          _this.$message({\n            message: \"回复成功\",\n            type: \"success\"\n          });\n\n          window.location.reload();\n        } else {\n          _this.$message.error(\"fail\");\n        }\n      });\n    },\n    //无用表单\n    UploadWorkerEva: function UploadWorkerEva() {},\n    //评论回复表单\n    UploadLM: function UploadLM() {\n      this.comment_visible = false;\n      var vo = {\n        reply: this.form_comment.content,\n        id: this.user_comment[this.state_index].id\n      };\n      console.log(vo);\n      this.UploadMyLM(vo);\n    },\n    //  查看是否登录\n    isLogin: function isLogin() {\n      var _this2 = this;\n\n      if (localStorage.getItem(\"WID\") == null) {\n        this.$confirm('未检测到登录', '提示', {\n          confirmButtonText: '去登陆',\n          cancelButtonText: '取消',\n          type: 'warning'\n        }).then(function () {\n          _this2.$router.push(\"/worker_login\");\n        });\n      } else {\n        return true;\n      }\n    },\n    //显示我的评论界面\n    ShowMyComment: function ShowMyComment() {\n      if (this.isLogin()) {\n        this.My_comment_visible = true;\n      }\n    },\n    //显示添加回复\n    add_leavingMessage: function add_leavingMessage(index) {\n      if (this.isLogin()) {\n        this.state_index = index;\n        this.comment_visible = true;\n      }\n    },\n    //更改样式设定\n    changeSatisfyCss: function changeSatisfyCss(item) {\n      if (item.type == \"非常满意\") {\n        return this.exe;\n      }\n\n      if (item.type == \"一般\") {\n        return this.just;\n      }\n\n      if (item.type == \"不满意\") {\n        return this.awful;\n      }\n    },\n    //查询员工接口和我的评价并且放入state_member\n    QueryWorkerApi: function QueryWorkerApi() {\n      var _this3 = this;\n\n      QueryWorker().then(function (res) {\n        if (res.code == 200) {\n          for (var i = 0; i < res.data.length; i++) {\n            if (parseInt(localStorage.getItem(\"WID\")) == res.data[i].id) {\n              _this3.state_member = res.data[i];\n            }\n          }\n\n          console.log(_this3.state_member);\n          QueryWorkerMycomment().then(function (res) {\n            if (res.code == 200) {\n              for (var i = 0; i < res.data.length; i++) {\n                //   用来验证与本地存储的数据是否一致\n                if (true) {\n                  _this3.Mycomment.push(res.data[i]);\n                }\n              }\n            }\n          });\n        }\n      });\n    },\n    //查询影院评论接口并且放入user_comment\n    QueryCineme: function QueryCineme() {\n      var _this4 = this;\n\n      QueryCinemeComment().then(function (res) {\n        if (res.code == 200) {\n          for (var i = 0; i < res.data.length; i++) {\n            if (res.data[i].user != null) {\n              _this4.user_comment.push(res.data[i]);\n            }\n          } //转化其中显示的日期\n\n\n          for (var i = 0; i < _this4.user_comment.length; i++) {\n            _this4.user_comment[i].createTime = parseTime(new Date(_this4.user_comment[i].createTime));\n          }\n\n          ;\n        }\n      });\n    }\n  },\n  computed: {\n    //评论全部放入新数组的算法\n    Mycomment_new: function Mycomment_new() {\n      this.Mycomment_new_list = undefined;\n      this.Mycomment_new_list = new Array();\n\n      for (var j = 0; j < this.Mycomment.length; j++) {\n        if (this.state_member.id == this.Mycomment[j].wid) {\n          this.Mycomment_new_list.push(this.Mycomment[j]);\n        }\n      } //转化其中显示的日期\n\n\n      for (var i = 0; i < this.Mycomment_new_list.length; i++) {\n        this.Mycomment_new_list[i].createTime = parseTime(new Date(this.Mycomment_new_list[i].createTime), \"{y}-{m}-{d}\");\n      }\n\n      ;\n      return this.Mycomment_new_list;\n    }\n  },\n  beforeCreate: function beforeCreate() {\n    console.log(\"BeforeCreate\");\n    this.$store.state.navnum = 3;\n    this.$store.state.color4.color = \"blue\";\n    this.$store.state.color1.color = \"#5b5d63\";\n    this.$store.state.color2.color = \"#5b5d63\";\n    this.$store.state.color3.color = \"#5b5d63\";\n  },\n  created: function created() {\n    if (localStorage.getItem(\"WID\") == null) {\n      this.$message.error(\"没有登录\");\n      window.location.href = \"#/\";\n    }\n  },\n  mounted: function mounted() {\n    //查询评论接口\n    this.QueryCineme(); //查询员工列表和我的评论接口\n\n    this.QueryWorkerApi();\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiEA;AACA,+C,CAAA;;AACA;AACAA,MADA,kBACA;AACA;AACA;AACAC;AAAAC;AAAA,OAFA;AAGA;AACAC;AAAAD;AAAAE;AAAA,OAJA;AAKA;AACAC,4BANA;AAOA;AACAC,yCARA;AASA;AACAC,+BAVA;AAWA;AACAC;AAAAC;AAAAC;AAAAC;AAAA,OAZA;AAaAC;AAAAH;AAAAC;AAAAC;AAAA,OAbA;AAcAE;AAAAJ;AAAAC;AAAAC;AAAA,OAdA;AAeA;AACAG,oBAhBA;AAiBAC,sBAjBA;AAmBA;AACAC,sBApBA;AAqBA;AACAC,mBAtBA;AAuBA;AACAC;AAxBA;AA0BA,GA5BA;AA6BAC;AAEA;AACAC,cAHA,sBAGAC,EAHA,EAGA;AAAA;;AACAC;AACA;AACA;AAAAC;AAAAC;AAAA;;AACAC;AACA,SAHA,MAIA;AACA;AACA;AACA,OARA;AAQA,KAZA;AAcA;AACAC,mBAfA,6BAeA,EAfA;AAgBA;AACAC,YAjBA,sBAiBA;AACA;AACA;AACAC,wCADA;AAEAC;AAFA;AAIAC;AACA;AACA,KAzBA;AA0BA;AACAC,WA3BA,qBA2BA;AAAA;;AACA;AACA;AACAC,kCADA;AAEAC,gCAFA;AAGAT;AAHA,WAIAU,IAJA,CAIA;AACA;AACA,SANA;AAOA,OARA,MASA;AACA;AACA;AAEA,KAzCA;AA0CA;AACAC,iBA3CA,2BA2CA;AACA,0BACA;AACA;AACA;AACA,KAhDA;AAiDA;AACAC,sBAlDA,8BAkDAC,KAlDA,EAkDA;AACA,0BACA;AACA;AACA;AACA;AAAA,KAvDA;AAwDA;AACAC;AACA;AAAA;AAAA;;AACA;AAAA;AAAA;;AACA;AAAA;AAAA;AACA,KA7DA;AA8DA;AACAC,kBA/DA,4BA+DA;AAAA;;AACAC;AACA;AACA,oDACA;AACA;AACA;AACA;AACA;;AACAV;AACAW;AACA;AACA,wDACA;AACA;AACA,0BACA;AACA;AACA;AACA;AAEA;AACA,WAZA;AAaA;AACA,OAvBA;AAwBA,KAxFA;AAyFA;AACAC,eA1FA,yBA0FA;AAAA;;AACAC;AACA;AACA,oDACA;AACA;AACA;AACA;AACA,WANA,CAOA;;;AACA;AACA;AACA;;AAAA;AACA;AAEA,OAdA;AAeA;AA1GA,GA7BA;AA0IAC;AACA;AACAC;AACA;AACA;;AACA;AACA,2DACA;AACA;AACA;AACA,OARA,CASA;;;AACA;AACA;AACA;;AAAA;AACA;AAEA;AAjBA,GA1IA;AA6JAC;AACAhB;AACA;AACA;AACA;AAAA;AAAA;AACA,GAlKA;AAmKAiB;AACA,6CACA;AACA;AACAtB;AACA;AACA,GAzKA;AA0KAuB;AACA;AACA,uBAFA,CAGA;;AACA;AACA;AA/KA","names":["data","form_comment","content","form_comment_state_member","satisfy","comment_visible","comment_state_member_visible","My_comment_visible","exe","excellent","justso","bad","just","awful","state_index","state_member","user_comment","Mycomment","Mycomment_new_list","methods","UploadMyLM","vo","UploadLeaveMessageReply","message","type","window","UploadWorkerEva","UploadLM","reply","id","console","isLogin","confirmButtonText","cancelButtonText","then","ShowMyComment","add_leavingMessage","index","changeSatisfyCss","QueryWorkerApi","QueryWorker","QueryWorkerMycomment","QueryCineme","QueryCinemeComment","computed","Mycomment_new","beforeCreate","created","mounted"],"sourceRoot":"src/views","sources":["comments_worker.vue"],"sourcesContent":["<template>\r\n<div class=\"comment_box\">\r\n  <el-container>\r\n      <!-- 左侧客服栏位 -->\r\n      <el-aside>\r\n          <p class=\"p0\">我的信息</p>\r\n            <div class=\"state_member_card\" >\r\n              <img :src=\"state_member.avatar\" class=\"state_member_img\">\r\n              <p class=\"p1\" >名字:{{state_member.nickname}}</p>\r\n              <p class=\"p2\" >性别:{{state_member.gender}}</p>\r\n              <p class=\"p3\" >ID:{{state_member.id}}</p>\r\n              <p class=\"p3\" >部门:{{state_member.department}}</p>\r\n             <el-link class=\"btnMycomment\" :underline=\"false\" @click=\"ShowMyComment()\"> 我的评价</el-link>\r\n             <el-link class=\"btnMycomment\" style=\"display:block;width:100px;margin-top:20px;position:absolute;left:-100px\" :underline=\"false\" @click=\"ShowMyUpdate()\"> 修改个人信息</el-link>\r\n          </div>\r\n      </el-aside>\r\n      \r\n       <!-- 我的评价弹窗 -->\r\n      <el-dialog title=\"我的评价\" :visible.sync=\"My_comment_visible\" width=\"40%\"  >\r\n      <div class=\"mycomment\" style=\"border-top:none\"><span class=\"my_comment_span3\" >评论时间</span><span class=\"my_comment_span4\">评论</span></div>\r\n      <div class=\"mycomment\" v-for=\"(item,index) in Mycomment_new\" :key=\"index\"><span class=\"my_comment_span1\" >{{item.createTime}}</span><span class=\"my_comment_span2\">{{item.content}}</span><span :class=\"changeSatisfyCss(item)\">{{item.type}}</span></div>\r\n      \r\n      <br />\r\n     <span slot=\"footer\" class=\"dialog-footer\">\r\n     <el-button  @click=\"My_comment_visible = false\">关闭</el-button>\r\n  </span>\r\n</el-dialog>\r\n      <!-- 我的评价结束 -->\r\n\r\n       <!-- 左侧客服栏位结束 -->\r\n\r\n        <!-- 右侧影院留言栏位 -->\r\n      <el-main >\r\n          <p style=\"margin-left:50px;font-size:20px;font-weight:600;color:white\">影院留言</p>\r\n          <div class=\"user_card\" v-for=\"(item,index) in user_comment\" :key=\"index\">\r\n              <img :src=\"item.user.avatar\" class=\"user_img\">\r\n              <p class=\"LM1\" >客人:&nbsp;{{item.user.nickname}}</p>\r\n              <p class=\"LM2\" >{{item.content}}</p>\r\n              <p class=\"LM3\" >Time:{{item.createTime}}</p> \r\n              <p class=\"LM4\" >回复:{{item.reply}}</p>\r\n              <el-button style=\"display:inline-block;position: relative;left: 599px;\r\n               top: -90px;\" type=\"primary\" @click=\" add_leavingMessage(index)\">添加回复</el-button>\r\n          </div>\r\n          \r\n      </el-main>\r\n      <!-- 评论弹窗 -->\r\n      <el-dialog title=\"添加回复\" :visible.sync=\"comment_visible\" width=\"50%\"  >\r\n      <el-form :model=\"form_comment\">\r\n       <el-form-item label=\"\" >\r\n      <el-input v-model=\"form_comment.content\" autocomplete=\"off\" type=\"textarea\" maxlength=\"60\" show-word-limit :autosize=\"{ minRows: 10, maxRows: 10}\"\r\n        placeholder=\"请输入内容\"></el-input>\r\n    </el-form-item>\r\n    </el-form>\r\n     <span slot=\"footer\" class=\"dialog-footer\">\r\n     <el-button @click=\"comment_visible = false\">取 消</el-button>\r\n     <el-button type=\"primary\" @click=\"UploadLM()\">添加回复</el-button>\r\n  </span>\r\n</el-dialog>\r\n      <!-- 评论弹窗结束 -->\r\n       <!-- 右侧影院留言栏位结束 -->\r\n  </el-container>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport {QueryWorker,QueryCinemeComment,QueryWorkerMycomment,UploadWorkerMycomment,UploadLeaveMessage,UploadLeaveMessageReply} from \"@/api/comment\";\r\nimport { parseTime } from \"@/plugins/DateTran\";//日期格式转化\r\nexport default {\r\n  data(){\r\n      return{\r\n          //影院留言表单\r\n          form_comment:{content:\"\"},\r\n          //对员工评价表单\r\n          form_comment_state_member:{content:\"\",satisfy:\"一般\"},\r\n          //添加评论显示\r\n          comment_visible:false,\r\n            //员工评价显示\r\n          comment_state_member_visible:false,\r\n           //我对员工评价\r\n          My_comment_visible:false,\r\n          //满意度样式\r\n           exe:{excellent:true,justso:false,bad:false},\r\n           just:{excellent:false,justso:true,bad:false},\r\n           awful:{excellent:false,justso:false,bad:true},\r\n          //   存放员工顺序，让其显示不同按钮下的各个员工评论\r\n          state_index:0,\r\n          state_member:[\r\n          ],\r\n          //用户评论\r\n          user_comment:[],\r\n          //我的评论,接口输入\r\n          Mycomment:[],\r\n          //我的评论,筛选后\r\n          Mycomment_new_list:{},\r\n      }\r\n  },\r\n  methods:{\r\n      \r\n      //上传影院留言接口\r\n      UploadMyLM(vo){\r\n        UploadLeaveMessageReply(vo).then((res)=>{\r\n              if(res.code==200){\r\n                   this.$message({message:\"回复成功\",type:\"success\"})\r\n                   window.location.reload();\r\n             }\r\n             else{\r\n                 this.$message.error(\"fail\")\r\n             }\r\n                  })}\r\n      ,\r\n      //无用表单\r\n      UploadWorkerEva(){},\r\n      //评论回复表单\r\n      UploadLM(){\r\n          this.comment_visible = false;\r\n          const vo={\r\n              reply:this.form_comment.content,\r\n              id:this.user_comment[this.state_index].id,\r\n          }\r\n          console.log(vo);\r\n          this.UploadMyLM(vo);\r\n      },\r\n      //  查看是否登录\r\n    isLogin() {\r\n      if (localStorage.getItem(\"WID\") == null) {\r\n        this.$confirm('未检测到登录', '提示', {\r\n          confirmButtonText: '去登陆',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$router.push(\"/worker_login\");\r\n        });\r\n      }\r\n      else {\r\n          return true;\r\n      }\r\n\r\n    },\r\n    //显示我的评论界面\r\n      ShowMyComment(){\r\n          if(this.isLogin())\r\n          {\r\n          this.My_comment_visible=true;\r\n          }\r\n      },\r\n      //显示添加回复\r\n      add_leavingMessage(index){\r\n          if(this.isLogin())\r\n          {\r\n              this.state_index=index;\r\n           this.comment_visible=true;\r\n      }},\r\n      //更改样式设定\r\n      changeSatisfyCss:function(item){\r\n          if(item.type==\"非常满意\"){return this.exe}\r\n          if(item.type==\"一般\"){return this.just}\r\n          if(item.type==\"不满意\"){return this.awful}\r\n      },\r\n      //查询员工接口和我的评价并且放入state_member\r\n      QueryWorkerApi(){\r\n          QueryWorker().then((res)=>{\r\n              if(res.code==200){\r\n                  for(var i=0;i<res.data.length;i++)\r\n                  {\r\n                      if(parseInt(localStorage.getItem(\"WID\"))==res.data[i].id){\r\n                          this.state_member=res.data[i];\r\n                      }\r\n                  }\r\n                  console.log(this.state_member);\r\n                  QueryWorkerMycomment().then((res)=>{\r\n                      if(res.code==200){\r\n                          for(var i=0;i<res.data.length;i++)\r\n                          {\r\n                            //   用来验证与本地存储的数据是否一致\r\n                              if(true)\r\n                              {\r\n                                  this.Mycomment.push(res.data[i]);\r\n                              }\r\n                          }\r\n                          \r\n                      }\r\n                  })\r\n              }\r\n          })\r\n      },\r\n      //查询影院评论接口并且放入user_comment\r\n      QueryCineme(){\r\n          QueryCinemeComment().then((res)=>{\r\n              if(res.code==200){\r\n                  for(var i=0;i<res.data.length;i++)\r\n                  {\r\n                      if(res.data[i].user!=null){\r\n                          this.user_comment.push(res.data[i]);\r\n                      }\r\n                  }\r\n                  //转化其中显示的日期\r\n                  for (var i = 0; i < this.user_comment.length; i++) {\r\n                  this.user_comment[i].createTime = parseTime(new Date(this.user_comment[i].createTime));\r\n                   };\r\n                  }\r\n              \r\n          })\r\n      }\r\n\r\n  },\r\n  computed:{\r\n      //评论全部放入新数组的算法\r\n      Mycomment_new:function(){\r\n           this.Mycomment_new_list=undefined;\r\n           this.Mycomment_new_list=new Array();\r\n              for(var j=0;j<this.Mycomment.length;j++){\r\n                  if(this.state_member.id==this.Mycomment[j].wid)\r\n                  {\r\n                      this.Mycomment_new_list.push(this.Mycomment[j]);\r\n                  }\r\n              }\r\n              //转化其中显示的日期\r\n                  for (var i = 0; i < this.Mycomment_new_list.length; i++) {\r\n                  this.Mycomment_new_list[i].createTime = parseTime(new Date(this.Mycomment_new_list[i].createTime),\"{y}-{m}-{d}\");\r\n                   };\r\n              return this.Mycomment_new_list;\r\n         \r\n      }\r\n  },\r\n  beforeCreate:function(){\r\n\t\t\tconsole.log(\"BeforeCreate\");\r\n            this.$store.state.navnum=3;\r\n            this.$store.state.color4.color=\"blue\";\r\n            this.$store.state.color1.color=\"#5b5d63\";this.$store.state.color2.color=\"#5b5d63\";this.$store.state.color3.color=\"#5b5d63\";\r\n\t\t},\r\n        created:function(){\r\n            if(localStorage.getItem(\"WID\")==null)\r\n            {\r\n                this.$message.error(\"没有登录\");\r\n               window.location.href=\"#/\";\r\n            }\r\n        },\r\n        mounted:function(){\r\n            //查询评论接口\r\n            this.QueryCineme();\r\n            //查询员工列表和我的评论接口\r\n            this.QueryWorkerApi();\r\n        }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 全局盒子 */\r\n\r\n.comment_box{\r\n    position: relative;\r\n    margin: 30px auto;\r\n    width:1400px;\r\n    height:700px;\r\n    overflow:auto\r\n}\r\n/* 客服栏 CSS */\r\n.p0{