\n      if (this.isLogin()) {\n        console.log(n);\n        this.state_index = n;\n        this.comment_state_member_visible = true;\n      }\n    },\n    //显示影院留言\n    add_leavingMessage: function add_leavingMessage() {\n      if (this.isLogin()) {\n        console.log();\n        this.comment_visible = true;\n      }\n    },\n    //更改样式设定\n    changeSatisfyCss: function changeSatisfyCss(item) {\n      if (item.type == \"非常满意\") {\n        return this.exe;\n      }\n\n      if (item.type == \"一般\") {\n        return this.just;\n      }\n\n      if (item.type == \"不满意\") {\n        return this.awful;\n      }\n    },\n    //查询员工接口和我的评价并且放入state_member\n    QueryWorkerApi: function QueryWorkerApi() {\n      var _this2 = this;\n\n      QueryWorker().then(function (res) {\n        if (res.code == 200) {\n          _this2.state_member = res.data;\n          QueryWorkerMycomment().then(function (res) {\n            if (res.code == 200) {\n              for (var i = 0; i < res.data.length; i++) {\n                //   用来验证与本地存储的数据是否一致\n                if (localStorage.getItem(\"uid\") == res.data[i].uid) {\n                  _this2.Mycomment.push(res.data[i]);\n                }\n              }\n            }\n          });\n        }\n      });\n    },\n    //查询影院评论接口并且放入user_comment\n    QueryCineme: function QueryCineme() {\n      var _this3 = this;\n\n      QueryCinemeComment().then(function (res) {\n        if (res.code == 200) {\n          for (var i = 0; i < res.data.length; i++) {\n            if (res.data[i].user != null) {\n              _this3.user_comment.push(res.data[i]);\n            }\n          } //转化其中显示的日期\n\n\n          for (var i = 0; i < _this3.user_comment.length; i++) {\n            _this3.user_comment[i].createTime = parseTime(new Date(_this3.user_comment[i].createTime));\n          }\n\n          ;\n        }\n      });\n    }\n  },\n  computed: {\n    //评论全部放入新数组的算法\n    Mycomment_new: function Mycomment_new() {\n      this.Mycomment_new_list = undefined;\n      this.Mycomment_new_list = new Array();\n\n      for (var j = 0; j < this.Mycomment.length; j++) {\n        if (this.state_member[this.state_index].id == this.Mycomment[j].wid) {\n          this.Mycomment_new_list.push(this.Mycomment[j]);\n        }\n      } //转化其中显示的日期\n\n\n      for (var i = 0; i < this.Mycomment_new_list.length; i++) {\n        this.Mycomment_new_list[i].createTime = parseTime(new Date(this.Mycomment_new_list[i].createTime), \"{y}-{m}-{d}\");\n      }\n\n      ;\n      return this.Mycomment_new_list;\n    }\n  },\n  beforeCreate: function beforeCreate() {\n    console.log(\"BeforeCreate\");\n    this.$store.state.navnum = 3;\n    this.$store.state.color4.color = \"blue\";\n    this.$store.state.color1.color = \"#5b5d63\";\n    this.$store.state.color2.color = \"#5b5d63\";\n    this.$store.state.color3.color = \"#5b5d63\";\n  },\n  mounted: function mounted() {\n    //查询评论接口\n    this.QueryCineme(); //查询员工列表和我的评论接口\n\n    this.QueryWorkerApi();\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsFA;AACA,+C,CAAA;;AACA;AACAA,MADA,kBACA;AACA;AACA;AACAC;AAAAC;AAAA,OAFA;AAGA;AACAC;AAAAD;AAAAE;AAAA,OAJA;AAKA;AACAC,4BANA;AAOA;AACAC,yCARA;AASA;AACAC,+BAVA;AAWA;AACAC;AAAAC;AAAAC;AAAAC;AAAA,OAZA;AAaAC;AAAAH;AAAAC;AAAAC;AAAA,OAbA;AAcAE;AAAAJ;AAAAC;AAAAC;AAAA,OAdA;AAeA;AACAG,oBAhBA;AAiBAC,qBACA;AACAC,6CADA;AAEAC,mBAFA;AAGAC,gBAHA;AAIAC,wBAJA;AAKAC,oBACA;AACAC,0BADA;AAEAnB,yBAFA;AAGAE;AAHA,SADA,EAMA;AACAiB,2BADA;AAEAnB,0BAFA;AAGAE;AAHA,SANA;AALA,OADA,EAmBA;AACAY,6CADA;AAEAC,mBAFA;AAGAC,gBAHA;AAIAC,wBAJA;AAKAC,oBACA;AACAC,0BADA;AAEAnB,4BAFA;AAGAE;AAHA,SADA,EAMA;AACAiB,2BADA;AAEAnB,0BAFA;AAGAE;AAHA,SANA;AALA,OAnBA,EAqCA;AACAY,6CADA;AAEAC,mBAFA;AAGAC,gBAHA;AAIAC,wBAJA;AAKAC,oBACA;AACAC,4BADA;AAEAnB,yBAFA;AAGAE;AAHA,SADA,EAMA;AACAiB,2BADA;AAEAnB,2BAFA;AAGAE;AAHA,SANA;AALA,OArCA,EAuDA;AACAY,6CADA;AAEAC,mBAFA;AAGAC,gBAHA;AAIAC,wBAJA;AAKAC,oBACA;AACAC,0BADA;AAEAnB,4BAFA